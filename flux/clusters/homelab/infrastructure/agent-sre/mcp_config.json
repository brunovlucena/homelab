{
  "mcpServers": {
    "sre-agent": {
      "command": "mcp-remote",
      "args": [
        "http://127.0.0.1:30121/mcp/?sreApiKey=sre-dev-your-key"
      ],
      "env": {}
    },
    "sre-agent-proper": {
      "command": "python",
      "args": [
        "mcp_server_proper.py"
      ],
      "env": {
        "SRE_API_KEY": "sre-dev-your-key",
        "MCP_HOST": "0.0.0.0",
        "MCP_PORT": "30121"
      }
    },
  }
}
